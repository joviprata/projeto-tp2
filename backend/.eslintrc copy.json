{
  "extends": [
    "eslint:recommended",
    "google",
    "prettier"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "require-jsdoc": "off"
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  }
}